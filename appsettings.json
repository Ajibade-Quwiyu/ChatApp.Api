{
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTION_STRING}"
  },
  "JWT": {
    "Key": "${JWT_KEY}"
  },
  "Supabase": {
    "Url": "${SUPABASE_URL}",
    "AnonKey": "${SUPABASE_ANON_KEY}",
    "ServiceRoleKey": "${SUPABASE_SERVICE_ROLE_KEY}"
  },
  "Cors": {
    "AllowedOrigins": "${ALLOWED_ORIGINS}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}